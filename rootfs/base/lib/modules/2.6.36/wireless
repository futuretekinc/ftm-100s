config wifi-device ath0 
  option type     ar9380
  option country  1
  option channel  6 
  option txpower  100
  option beacon_int 100
  option repeater 0
  option disabled 0 

config wifi-iface
  option device   ath0 
  option network  lan
  option mode     ap
  option hwmode   G 
  option ssid     ika_test 
  option hidden   0
  option wds_enable 0
  option wps_enable 0
  option wps_pin  00000000
  option auth	        wep-shared
  option encryption     40-bit 
  option wep_key_mode   hex 
  option wep_key	1
  option wep_key1	  1234567890
  option wep_key2	  1234567890
  option wep_key3	  1234567890
  option wep_key4	  1234567890
    

config wifi-wds
  option device	  'wlan0-wds0'
