#!/bin/sh
#
# 1. Make an entry into ucitrack for this application
# 2. Make sure the application for this gui is enabled
#
[ -n "${IPKG_INSTROOT}" ] || {
  ( . /etc/uci-defaults/luci-app-transmission ) && rm -f /etc/uci-defaults/luci-app-transmission
  /etc/init.d/transmission enabled || /etc/init.d/transmission enable
}
